{
    "auth": {
      "identityProviders": {
        "customOpenIdConnectProviders": {
          "b2c": {
            "registration": {
              "clientIdSettingName": "B2C_CLIENT_ID",
              "clientCredential": {
                "clientSecretSettingName": "B2C_CLIENT_SECRET"
              },
              "openIdConnectConfiguration": {
                "wellKnownOpenIdConfiguration": "https://miyagawabtoc1.b2clogin.com/miyagawabtoc1.onmicrosoft.com/B2C_1_signin/v2.0/.well-known/openid-configuration"
              }
            },
            "login": {
              "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
              "scopes": [],
              "loginParameterNames": []
            }
          }
        }
      }
    },
    "routes": [
      {
        "route": "/*",
        "allowedRoles": [
          "authenticated"
        ]
      }
    ],
    "responseOverrides": {
      "401": { 
        "redirect": "/.auth/login/b2c",
        "statusCode": 302
      }
    },
    "navigationFallback": {
      "rewrite": "/"
    },
    "globalHeaders": {
      "X-Frame-Options" : "deny",
      "X-Content-Type-Options": "nosniff"
    }

  }
  